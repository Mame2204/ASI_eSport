@page
@model ASI_eSport.Pages.Contenants.CreateModel

@{
    ViewData["Title"] = "Create";
}

<h1>Create</h1>

<h4>Contenir</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form method="post">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="contenir.LEquipeID" value="@Model.equipe.ID" />
            <div class="form-group">
                <label asp-for="contenir.LeLicencieID" class="control-label"></label>
                <select asp-for="contenir.LeLicencieID" class="form-control" asp-items="ViewBag.LeLicencieID"></select>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-page="Index">Back to List</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
